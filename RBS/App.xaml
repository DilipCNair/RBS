<Application x:Name="Application_RBS" x:Class="RBS.App" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"	
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="Application_RBS_Startup"	
             Exit="Application_RBS_Exit"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="RBSResourceDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
